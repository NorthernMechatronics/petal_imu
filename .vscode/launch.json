{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cortex-debug",
            "request": "launch",
            "name": "Petal IMU Debug Launch",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/PETAL_IMU_Debug/${workspaceFolderBasename}_PETAL_IMU_Debug.axf",
            "servertype": "pyocd",
            "device": "AMA3B1KK-KBR",
            "interface": "swd",
            "serialNumber": "",
            "svdFile": "${workspaceFolder}/nmsdk2/hal/ambiq/pack/SVD/apollo3.svd",
            "showDevDebugOutput": "none",
            "serverArgs": [
                "--target=ama3b1kk_kbr",
                "--frequency=8MHz"
            ],
            "preLaunchTask": "CMake: build",
        },
        {
            "type": "cortex-debug",
            "request": "attach",
            "name": "Petal IMU Debug Attach",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/PETAL_IMU_Debug/${workspaceFolderBasename}_PETAL_IMU_Debug.axf",
            "servertype": "pyocd",
            "device": "AMA3B1KK-KBR",
            "interface": "swd",
            "serialNumber": "",
            "svdFile": "${workspaceFolder}/nmsdk2/hal/ambiq/pack/SVD/apollo3.svd",
            "showDevDebugOutput": "none",
            "serverArgs": [
                "--target=ama3b1kk_kbr",
                "--frequency=8MHz"
            ],
            "preLaunchTask": "CMake: build",
        }
    ]
}